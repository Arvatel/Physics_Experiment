<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.arvatel.physicsexperiment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_atom"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_atom"
        android:supportsRtl="true"
        android:theme="@style/Theme.PhysicsExperiment">
        <activity android:name=".ResultsActivity"></activity>
        <activity android:name=".StepActivity" />
        <activity android:name=".DescriptionActivity" />
        <activity android:name=".Main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>